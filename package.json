{
  "name": "@takwimuafrica/takwimu.africa",
  "version": "0.1.0-alpha",
  "private": true,
  "homepage": "https://dev.takwimu.africa",
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@codeforafrica/hurumap-ui": "^0.1.10",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.3",
    "@react-pdf/renderer": "^1.5.6",
    "@types/react": "^16.9.2",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "dom-to-image": "^2.6.0",
    "graphql": "^14.5.3",
    "graphql-tag": "^2.10.1",
    "html2canvas": "^1.0.0-rc.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.4",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.1",
    "react-pdf-js": "^5.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-share": "^3.0.0",
    "shortid": "^2.2.15",
    "typescript": "*"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.4",
    "prettier": "^1.16.1",
    "source-map-explorer": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn applyIdsToChartDefinitions && git add src/data/chart.json && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,scss,md}": [
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "commitChartDefinitions": "git add src/data/chart.json && git commit -m 'updated chart ids' && git push",
    "applyIdsToChartDefinitions": "node scripts/chartsDefinitionsApplyUniqueId.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "yarn applyIdsToChartDefinitions",
    "start": "react-scripts start",
    "prebuild": "yarn lint && yarn applyIdsToChartDefinitions",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn eslint --fix --ignore-path './.gitignore' --ext '.js,.json,.md' './'",
    "predeploy": "yarn build && yarn commitChartDefinitions",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "defaults"
  ]
}
